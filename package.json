{
  "name": "@wfcd/arsenal-parser",
  "version": "0.0.1",
  "description": "Parser for Digital Extreme's Twitch Arsenal Extension data",
  "main": "src/ArsenalParser.js",
  "types": "./types/src/ArsenalParser.d.ts",
  "public": true,
  "files": [
    "src",
    "LICENSE",
    "README.md",
    "types"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/node-fetch": "^3.0.3",
    "chai": "^4.3.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^9.0.2",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "test": "nyc mocha ",
    "coverage": "npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "prepublishOnly": "npm run build:types",
    "build:docs": "jsdoc -c .github/docs/conf.json -d docs",
    "build:types": "tsc -p tsconfig.declaration.json"
  },
  "keywords": [
    "warframe",
    "parser",
    "twitch",
    "arsenal"
  ],
  "author": "NicksWorld",
  "contributors": [
    "TobiTenno <tobiah@pm.me>"
  ],
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": "https://github.com/wfcd/arsenal-parser",
  "engines": {
    "node": ">=12.18.3",
    "npm": ">=6.14.6"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "valid-jsdoc": [
        "error",
        {
          "requireReturn": false,
          "requireReturnDescription": false,
          "preferType": {
            "String": "string",
            "Number": "number",
            "Boolean": "boolean",
            "Function": "function",
            "object": "Object",
            "date": "Date",
            "error": "Error"
          },
          "prefer": {
            "return": "returns"
          }
        }
      ],
      "no-underscore-dangle": "off",
      "strict": [
        "error",
        "safe"
      ],
      "linebreak-style": "off",
      "import/no-unresolved": 0
    }
  },
  "mocha": {
    "exit": true,
    "spec": "test/**/*.spec.js"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "skip-full": true
  },
  "dependencies": {
    "warframe-items": "1.x"
  }
}
